{
  "name": "@aws-sdk-notes-app/frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend for notes app created using modular AWS SDK for JavaScript",
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.15.0",
    "@aws-sdk/client-polly": "3.15.0",
    "@aws-sdk/client-s3": "3.15.0",
    "@aws-sdk/client-transcribe-streaming": "3.15.0",
    "@aws-sdk/credential-provider-cognito-identity": "3.15.0",
    "@aws-sdk/polly-request-presigner": "3.15.0",
    "@aws-sdk/s3-request-presigner": "3.15.0",
    "@aws-sdk/util-create-request": "3.15.0",
    "@aws-sdk/util-format-url": "3.15.0",
    "@reach/router": "1.3.4",
    "microphone-stream": "5.2.0",
    "react": "17.0.1",
    "react-bootstrap": "1.4.0",
    "react-dom": "17.0.1"
  },
  "scripts": {
    "prepare:frontend": "cd .. && yarn prepare:frontend",
    "build": "react-scripts build",
    "build:frontend": "cd .. && yarn build:frontend",
    "start": "react-scripts start",
    "start:frontend": "cd .. && yarn start:frontend",
    "serve": "ws -d build -o",
    "serve:frontend": "cd .. && yarn serve:frontend",
    "cdk": "cd .. && yarn cdk"
  },
  "keywords": [
    "modular",
    "aws",
    "JavaScript",
    "TypeScript",
    "SDK",
    "v3"
  ],
  "author": "Kamat, Trivikram",
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^14.14.2",
    "@types/reach__router": "1.3.6",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "local-web-server": "^4.2.1",
    "react-bootstrap-icons": "1.3.0",
    "react-scripts": "4.0.1",
    "typescript": "~4.1.2"
  }
}
